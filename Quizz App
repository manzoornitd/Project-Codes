const st=require('readline-sync');
console.log("     Wellcome to Math Quizz");
function f1()
{
  console.log("     Calculate the value of 6-3/3");
  console.log("  options are:");
  console.log("  1. 0               2. 1");
  console.log("  3. 5               4. none");
  let v1;
  vi=st.question();
  if(vi==3)
  {
    console.log("\nCorrect Answer\n");
    return 1;
  }
  else
  {
    console.log("\nWrong answer\n");
    return 0;
  }
}

function f2()
{
  console.log("     Calculate the value of 12*12-10");
  console.log("  options are:");
  console.log("  1. 114               2. 134");
  console.log("  3. 24               4. none");
  let v1;
  vi=st.question();
  if(vi==1)
  {
    console.log("\nCorrect Answer\n");
    return 1;
  }
  else
  {
    console.log("\nWrong answer\n");
    return 0;
  }
}
  function f3()
