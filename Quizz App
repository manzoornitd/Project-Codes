function f4()
{
  console.log("     Calculate the value of 3-3^2*2");
  console.log("  options are:");
  console.log("  1. -15               2. -78");
  console.log("  3.  5                4. none");
  let v1;
  vi=st.question();
  if(vi==1)
  {
    console.log("\nCorrect Answer\n");
    return 1;
  }
  else
  {
    console.log("\nWrong answer\n");
    return 0;
  }
}
function f5()
{
  console.log("     Calculate the value of 1+2+3+....+10");
  console.log("  options are:");
  console.log("  1. 65               2. 54");
  console.log("  3. 55               4. none");
  let v1;
  vi=st.question();
  if(vi==3)
  {
    console.log("\nCorrect Answer\n");
    return 1;
  }
  else
  {
    console.log("\nWrong answer\n");
    return 0;
  }
}

var count=0;
count=count+f1();
count=count+f2();
count=count+f3();
count=count+f4();
count=count+f5();
console.log(`your percentage is ${count*20} %` );
